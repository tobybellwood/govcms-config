{
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.6.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.6.2/chosen_v1.6.2.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/harvesthq/chosen/releases/",
                    "type": "git",
                    "reference": "tag/v1.6.2"
                }
            }
        }
    ],
    "require": {
        "drupal/core": "8.3.0",
        "acquia/lightning": "^2.1.0",
