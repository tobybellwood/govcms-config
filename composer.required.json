        "drupal/search_api_page": "^1.0@alpha",
        "drupal/search_api_solr": "^1.0@beta",
        "drupal/seckit": "^1.0@alpha",
        "drupal/security_review": "1.x-dev",
        "drupal/select_or_other": "^1.0@alpha",
        "drupal/shield": "^1.0",
        "drupal/simple_sitemap": "^2.8",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/taxonomy_menu": "^3.3",
        "drupal/token": "^1.0@RC",
        "drupal/typed_data": "^1.0@alpha",
        "drupal/username_enumeration_prevention": "^1.0@beta",
        "drupal/views_bootstrap": "3.x-dev",
        "drupal/video_embed_field": "^1.4",
        "drupal/view_unpublished": "^1.0@alpha",
        "drupal/views_data_export": "^1.0@alpha",
        "drupal/views_infinite_scroll": "^1.3",
        "drupal/webform": "^5.0@beta",
        "drupal/workbench_moderation": "^1.2",
        "drupal/workspace": "^1.0@alpha",
        "govcms/govcms-ui": "dev-master",
        "harvesthq/chosen": "1.6.2",
        "drush/drush": "^8.1.10"
    },
    "require-dev": {
        "asm89/twig-lint": "^1.0.2",
        "behat/behat": "^3.1",
        "behat/mink": "~1.7",
        "behat/mink-selenium2-driver": "^1.3.1",
        "bex/behat-screenshot": "^1.2",
        "drupal/console": "^1.0.0-rc13",
        "drupal/devel": "^1.0@RC",
        "drupal/drupal-extension": "^3.2",
        "grasmash/drupal-security-warning": "^1.0.0",
        "drupal-composer/drupal-scaffold": "^2.1.0",
        "jarnaiz/behat-junit-formatter": "^1.3.2",
        "se/selenium-server-standalone": "^2.53",
        "jakoch/phantomjs-installer":   "2.1.1-p07"
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            },
            "excludes": [
                "sites/development.services.yml"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "Clear Twig caches on deploys": "https://www.drupal.org/files/issues/no_reliable_method-2752961-29.patch",
                "Improve the Views integration for DateRange fields": "https://www.drupal.org/files/issues/0001-Issue-2786577-Improve-the-Views-integration-for-Date_3.patch"
            }
        }
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock",
            "@composer clearcache",
            "@composer install"
        ],
        "install-phantomjs": [
            "rm -rf vendor/bin/phantomjs",
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    }
}
